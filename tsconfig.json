{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"outDir": "./dist",
		"strict": true
	},
	"include": [
		"src/**/*.ts",
		"src/server.js",
		"src/tempTypes.js",
		"src/session.js",
		"src/helpers.js",
		"src/constants.js"
	],
	"exclude": ["node_modules"]
}
